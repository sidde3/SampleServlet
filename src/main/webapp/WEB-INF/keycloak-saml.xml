<keycloak-saml-adapter>
    <SP entityID="SampleServlet"
        sslPolicy="EXTERNAL"
        logoutPage="SPECIFY YOUR LOGOUT PAGE!">
        <Keys>
            <Key signing="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
                <CertificatePem>
                    
                </CertificatePem>
            </Key>
            <Key encryption="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
            </Key>
        </Keys>
        <IDP entityID="idp"
             signatureAlgorithm="RSA_SHA256">
            <SingleSignOnService signRequest="true"
                                 validateResponseSignature="true"
                                 validateAssertionSignature="true"
                                 requestBinding="POST"
                                 bindingUrl="http://localhost:8080/auth/realms/saml-realm/protocol/saml"/>
            <SingleLogoutService signRequest="true"
                                 signResponse="true"
                                 validateRequestSignature="true"
                                 validateResponseSignature="true"
                                 requestBinding="POST"
                                 responseBinding="POST"
                                 postBindingUrl="http://localhost:8080/auth/realms/saml-realm/protocol/saml"
                                 redirectBindingUrl="http://localhost:8080/auth/realms/saml-realm/protocol/saml"/>
        </IDP>
    </SP>
</keycloak-saml-adapter>
